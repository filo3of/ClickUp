{
  "name": "clickup",
  "version": "1.0.0",
  "description": "Cypress automation tests with page object modal approach",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open --e2e --browser chrome",
    "cy:run": "npx cypress run --e2e --browser chrome --headless",
    "current:open": "npx cypress run --e2e --browser chrome --env grepTags=@current",
    "current:run": "npx cypress run --e2e --browser chrome --headless --env grepTags=@current"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filo3of/ClickUp.git"
  },
  "author": "Marko Bogicevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filo3of/ClickUp/issues"
  },
  "homepage": "https://github.com/filo3of/ClickUp#readme",
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "cypress": "^13.14.2",
    "cypress-mochawesome-reporter": "^3.8.2"
  }
}

{
  "name": "clickup",
  "version": "1.0.0",
  "description": "Cypress automation tests with page object modal approach",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open --e2e --browser chrome",
    "cy:run": "npx cypress run --e2e --browser chrome --headless",
    "current:open": "npx cypress open --e2e --browser chrome --env grepTags=@current",
    "current:run": "npx cypress run --e2e --browser chrome --headless --env grepTags=@current",
    "e2e:open": "npx cypress open --e2e --browser chrome --env grepTags=@e2e",
    "e2e:run": "npx cypress run --e2e --browser chrome --headless --env grepTags=@e2e",
    "smoke:open": "npx cypress open --e2e --browser chrome --env grepTags=@smoke",
    "smoke:run": "npx cypress run --e2e --browser chrome --headless --env grepTags=@smoke",
    "api:open": "npx cypress open --e2e --browser chrome --env grepTags=@api",
    "api:run": "npx cypress run --e2e --browser chrome --headless --env grepTags=@api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filo3of/ClickUp.git"
  },
  "author": "Marko Bogicevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filo3of/ClickUp/issues"
  },
  "homepage": "https://github.com/filo3of/ClickUp#readme",
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "cypress": "^13.14.2",
    "cypress-if": "^1.12.6",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-plugin-api": "^2.11.2",
    "cypress-plugin-steps": "^1.1.1",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1"
  }
}
